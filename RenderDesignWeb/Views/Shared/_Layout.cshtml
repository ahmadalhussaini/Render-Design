<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="company Millimeter design studio works in many areas of design
 Interior Design | Architectural Solutions | Branding | Furniture Design | Lighting Design | Contracting | Implementation And Supervison
We work in Jordan, UAE and Saudi Arabia">

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Render Design</title>
    <link rel="icon" href="~/imges/logo3.PNG">
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="http://fortawesome.github.io/Font-Awesome/3.2.1/assets/font-awesome/css/font-awesome.css">
    <link href="~/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/Layout.css" rel="stylesheet" />
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg  navbar-dark bg-dark">

            <img src="~/imges/logo3.PNG" height="40" width="40" style="margin-right:5px" />
            <a class="navbar-brand" asp-controller="Home" asp-action="Home">Render Design </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>



            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item dropdown active">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Projects
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Commercial">Commercial</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Residential">Residential</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Landscape">Landscape</a>

                        </div>
                    </li>
                    <li class="nav-item dropdown active">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Designer
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Commercial">Commercial</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Residential">Residential</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Landscape">Landscape</a>

                        </div>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="Post"> Post  <span class="sr-only">(current)</span></a>
                    </li>



                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="ContactUs">Contact Us <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" data-bs-toggle="modal" data-bs-target="#exampleModal1"> Sign up  <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" data-bs-toggle="modal" data-bs-target="#exampleModal2"> Log in  <span class="sr-only">(current)</span></a>
                    </li>
                </ul>

            </div>
        </nav>
    </header>
    <!-- Button trigger modal -->
    <!-- Modal -->
    <div class="modal fade" id="exampleModal2" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <img src="~/imges/logo.png" style="width: 110px; height:90px" alt="logo"  />
                    <h5 class="modal-title" id="exampleModalLabel"> </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div >

                        
                    </div>

                    <form class="login-form" asp-controller="Auth" asp-action="Login">

                        <!-- Email input -->
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example3">Email Address</label>
                            <input type="email" id="form3Example3" class="form-control form-control-lg"
                                   placeholder="Enter a valid email address" />
                        </div>

                        <!-- Password input -->
                        <div class="form-outline mb-3">
                            <label class="form-label" for="form3Example4">Password</label>
                            <input type="password" id="form3Example4" class="form-control form-control-lg"
                                   placeholder="Enter password" />
                        </div>

                        <div class="d-flex justify-content-between align-items-center">
                            <!-- Checkbox -->
                            <div class="form-check mb-0">
                                <input class="form-check-input me-2" type="checkbox" value="" id="form2Example3" />
                                <label class="form-check-label" for="form2Example3">
                                    Remember me
                                </label>
                            </div>
                            <a href="#!" class="text-body">Forgot password?</a>
                        </div>

                        <div class="text-center text-lg-start mt-4 pt-2">
                            <button type="button" class="btn btn-primary mb-2"
                                    style="padding-left: 2.5rem; padding-right: 2.5rem;">
                                Login
                            </button>

                        </div>
                        <p class="message">Don't Have an Account'? <a href="reg.html">Register Here</a></p>

                    </form>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="exampleModal1" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <img src="~/imges/logo.png" style="width: 110px; height:90px" alt="logo" />
                    <h5 class="modal-title" id="exampleModalLabel"></h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">

                    <form class="register-form" asp-controller="Auth" asp-action="Register">


                        <!-- 2 column grid layout with text inputs for the first and last names -->
                        <div class="form-outline mb-4">

                            <div class="form-outline">
                                <label class="form-label" for="form3Example1">Name</label>
                                <input type="text" id="form3Example1" class="form-control" placeholder="Designer/Company Name" />
                            </div>
                        </div>

                        <!-- Email input -->
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example3">Email Address</label>
                            <input type="email" id="form3Example3" class="form-control" placeholder="Valid Email" />

                        </div>

                        <!-- Password input -->
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example4">Password</label>
                            <input type="password" id="form3Example4" class="form-control" placeholder="Enter Password" />

                        </div>
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example5">Password Confirmation</label>
                            <input type="password" id="form3Example5" class="form-control" placeholder="Confirm password" />
                        </div>
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example6">Phone Number</label>
                            <input type="text" name="PhoneNumber" id="form3Example6" class="form-control" placeholder="Add Phone Number" />
                        </div>
                        <div class="form-outline mb-4">
                            <label class="form-label" for="form3Example7">Date of Creation</label>
                            <input type="date" name="CreatedAt" id="form3Example7" class="form-control" placeholder="Create At" />
                        </div>
                        <button type="submit" class="btn btn-primary btn-block mb-4">
                            Sign up
                        </button>

                        <p class="message">Already Have an Account? <a href="Log.html">Login</a></p>

                    </form>
                </div>
             </div>
        </div>
    </div>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer style="height:60px;background-color:aquamarine">



        <div class="text-center">

            <p>
                Copyright &copy; 2022
                <i class="icon-heart color-danger" aria-hidden="true"></i> by Render Design

            </p>
        </div>

    </footer>
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
    <script>
        $(function () {
            $("#example1").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $('#example2').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });
            $('#example3').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });
            $("#example4").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $("#example5").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $("#example6").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
        });
    </script>
</body>
</html>