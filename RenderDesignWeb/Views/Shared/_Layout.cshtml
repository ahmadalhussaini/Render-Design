<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="company Millimeter design studio works in many areas of design
 Interior Design | Architectural Solutions | Branding | Furniture Design | Lighting Design | Contracting | Implementation And Supervison
We work in Jordan, UAE and Saudi Arabia">

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Render Design</title>
    <link rel="icon" href="~/imges/logo3.PNG">
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="http://fortawesome.github.io/Font-Awesome/3.2.1/assets/font-awesome/css/font-awesome.css">
    <link href="~/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/Layout.css" rel="stylesheet" />
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg  navbar-dark bg-dark">

            <img src="~/imges/logo3.PNG" height="40" width="40" style="margin-right:5px" />
            <a class="navbar-brand" asp-controller="Home" asp-action="Home">Render Design </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>



            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item dropdown active">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Projects
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Commercial">Commercial</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Residential">Residential</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Landscape">Landscape</a>

                        </div>
                    </li>
                    <li class="nav-item dropdown active">
                        <a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Designer
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Commercial">Commercial</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Residential">Residential</a>
                            <a class="dropdown-item" asp-controller="Home" asp-action="Projects" asp-route-type="Landscape">Landscape</a>

                        </div>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="AboutUs"> Post  <span class="sr-only">(current)</span></a>
                    </li>



                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="ContactUs">Contact Us <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" data-bs-toggle="modal" data-bs-target="#exampleModal1"> Sign up  <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" data-bs-toggle="modal" data-bs-target="#exampleModal2"> Log in  <span class="sr-only">(current)</span></a>
                    </li>
                </ul>

            </div>
        </nav>
    </header>
    <!-- Button trigger modal -->
    <!-- Modal -->
    <div class="modal fade" id="exampleModal2" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">

                    <form class="login-form" asp-controller="Auth" asp-action="Login">

                        <center><h1 class="main-heading">Login Form</h1></center>

                        <input type="text" name="Email" placeholder="user name" />

                        <input type="password" name="Password" placeholder="password" />

                        <button>LOGIN</button>

                        <p class="message">Not Registered? <a href="reg.html">Register</a></p>

                    </form>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="exampleModal1" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">

                    <form class="register-form" asp-controller="Auth" asp-action="Register">

                        <center><h1 class="main-heading">Register Form</h1></center>

                        <input type="text" name="Name" placeholder="Name Designer/Company" />

                        <input type="text" name="Email" placeholder="email-id" />

                        <input type="password" name="Password" placeholder="password" />

                        <input type="password" placeholder="Confirm password" />

                        <input type="date" name="CreatedAt" placeholder="Creat At" />

                        <button>SUBMIT</button>

                        <p class="message">Already Registered? <a href="Log.html">Login</a></p>

                    </form>
                </div>
             </div>
        </div>
    </div>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer style="height:60px;background-color:aquamarine">



        <div class="text-center">

            <p>
                Copyright &copy; 2022
                <i class="icon-heart color-danger" aria-hidden="true"></i> by Render Design

            </p>
        </div>

    </footer>
    <!-- JavaScript Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
    <script>
        $(function () {
            $("#example1").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $('#example2').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });
            $('#example3').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": false,
                "ordering": true,
                "info": true,
                "autoWidth": false,
                "responsive": true,
            });
            $("#example4").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $("#example5").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
            $("#example6").DataTable({
                "responsive": true,
                "autoWidth": false,
            });
        });
    </script>
</body>
</html>